{
	"name": "dot-net-vuejs-template",
	"version": "1.0.0",
	"description": "Template for a .NET/VueJS Application",
	"repository": {
		"type": "git",
		"url": "https://github.com/DanielJAch/dot-net-vuejs-template.git"
	},
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --content-base ./app/webroot/app --inline --hot --port 8086",
		"watch": "webpack --watch",
		"build": "webpack --config ./ClientApp/build/webpack.dev.config.js",
		"build-test": "webpack --config ./ClientApp/build/webpack.test.config.js",
		"build-staging": "webpack --config ./ClientApp/build/webpack.staging.config.js",
		"build-prod": "webpack --config ./ClientApp/build/webpack.prod.config.js",
		"build:watch": "webpack --watch --config ./ClientApp/build/webpack.dev.config.js",
		"clean": "rmdir /s /q node_modules",
		"reinstall": "npm run clean && npm install",
		"rebuild": "npm run clean && npm install && npm run build",
		"rebuild-test": "npm run clean && npm install && npm run build-test",
		"rebuild-staging": "npm run clean && npm install && npm run build-staging",
		"rebuild-prod": "npm run clean && npm install && npm run build-prod"
	},
	"author": "danieljach@gmail.com",
	"dependencies": {
		"axios": "0.27.2",
		"body-parser": "1.20.0",
		"bootstrap-vue": "2.22.0",
		"express": "4.18.1",
		"@fortawesome/fontawesome-free": "6.1.2",
		"helmet": "6.0.0",
		"jsonwebtoken": "8.5.1",
		"lodash": "4.17.21",
		"moment": "2.29.4",
		"node-cache": "5.1.2",
		"vue": "3.2.37",
		"vue-router": "4.1.5",
		"vuex": "4.0.2"
	},
	"devDependencies": {
		"acorn": "8.8.0",
		"babel-eslint": "10.1.0",
		"clean-webpack-plugin": "4.0.0",
		"compression-webpack-plugin": "10.0.0",
		"css-loader": "6.7.1",
		"eslint": "8.22.0",
		"eslint-friendly-formatter": "4.0.1",
		"eslint-loader": "4.0.2",
		"extract-text-webpack-plugin": "4.0.0-beta.0",
		"file-loader": "6.2.0",
		"friendly-errors-webpack-plugin": "1.7.0",
		"html-webpack-plugin": "5.5.0",
		"mini-css-extract-plugin": "2.6.1",
		"node-sass": "7.0.1",
		"sass-loader": "13.0.2",
		"terser-webpack-plugin": "5.3.5",
		"url-loader": "4.1.1",
		"vue-loader": "15.7.1",
		"vue-template-compiler": "2.7.10",
		"webpack": "5.74.0",
		"webpack-cli": "4.10.0",
		"webpack-merge": "5.8.0"
	}
}
